<h1>
  Gas_stations
</h1>
<table>
  <thead>
    <tr>
      <%GasStation.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.map(&:humanize).each do |field|%>
        <th><%= field %></th>

      <% end %>
    </tr>
  </thead>
  <tbody>
    <% GasStation.all.each do |gas_station| %> 
      <tr>
        <% GasStation.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.each do |field| %>
        
          <td><%= gas_station.send(field) %></td>
          
        <% end %>
      </tr>
    <% end %>

    
  </tbody>
</table>
