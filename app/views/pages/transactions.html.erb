<h1>
  Transaction
</h1>
<table>
  <thead>
    <tr>
      <%Transaction.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.map(&:humanize).each do |field|%>
        <th><%= field %></th>

      <% end %>
    </tr>
  </thead>
  <tbody>
    <% Transaction.all.each do |transaction| %> 
      <tr>
        <% Transaction.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.each do |field| %>
        
          <td><%= transaction.send(field) %></td>
          
        <% end %>
      </tr>
    <% end %>

    
  </tbody>
</table>
