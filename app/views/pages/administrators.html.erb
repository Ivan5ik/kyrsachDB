<h1>
  Administrator
</h1>
<%= link_to "Add new administrator", new_administrator_path, class: "small-btn create-btn" %>
<table>
  <thead>
    <tr>
      <%Administrator.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.map(&:humanize).each do |field|%>
        <th><%= field %></th>

      <% end %>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% Administrator.all.each do |administrator| %> 
      <tr>
        <% Administrator.column_names.reject { |col| ["id", "created_at", "updated_at"].include?(col) }.each do |field| %>
        
          <td><%= administrator.send(field) %></td>
          
        <% end %>
         <%= render partial:'reuseble/edit_delete',locals:{
          edit_path:edit_administrator_path(administrator),
          delete_path:administrator_path(administrator)}%>
      </tr>
    <% end %>

    
  </tbody>
</table>
